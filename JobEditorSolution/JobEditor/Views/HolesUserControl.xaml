        <Utils:ConverterDoubleToString x:Key="converterDoubleToString"/>
        <x:Array x:Key="FormatStringsDouble1" Type="{x:Type System:String}">
            <System:String>f2</System:String>
            <System:String>f4</System:String>
        </x:Array>
