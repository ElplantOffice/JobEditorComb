<CustomCtrlLib:Window x:Class="JobEditor.MainWindow"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:core="clr-namespace:System;assembly=mscorlib"
                       xmlns:sys="clr-namespace:System;assembly=mscorlib"
                       xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                       xmlns:CustomCtrlLib="clr-namespace:CustomControlLibrary;assembly=CustomControlLibrary"
                       xmlns:UserCtrlFileSelector="clr-namespace:UserControls.FileSelector;assembly=UserControls.FileSelector"
                       xmlns:UserCtrlLogin="clr-namespace:UserControls.Login;assembly=UserControls.Login"
                       xmlns:Utils="clr-namespace:Utils;assembly=Utils"
                       xmlns:Local="clr-namespace:JobEditor"
                       xmlns:Product="clr-namespace:ProductLib;assembly=ProductLib"
                       xmlns:JobEditorControls="clr-namespace:JobEditor.Views"
                       xmlns:Command="clr-namespace:JobEditor.Common"
                       mc:Ignorable="d"
                       StateChanged="Window_StateChanged" Closed="Window_Closed"
                       Name="wndJobEditor" 
                       Title="JobEditor"
                       Stylus.IsPressAndHoldEnabled="false" Height="768" Width="1366" MaxWidth="1366" MaxHeight="768"
                       Background="#204653" WindowStyle="None" ResizeMode="NoResize" Top="0" Left="0">
    
    <FrameworkElement.Resources>
        <Utils:BoolToVisibility x:Key="BoolToVisibility"/>
        <Local:ConverterEMeasuringTypeSelToImage x:Key="ConverterEMeasuringTypeSelToImage"/>
        <Local:ConverterEHeightCorrTypeSelToImage x:Key="ConverterEHeightCorrTypeSelToImage"/>
        <Utils:ConverterDoubleToString x:Key="converterDoubleToString"/>
        <x:Array x:Key="FormatStringsDouble1" Type="{x:Type sys:String}">
            <sys:String>f2</sys:String>
            <sys:String>f4</sys:String>
        </x:Array>
        <x:Array x:Key="FormatStringsDouble2" Type="{x:Type sys:String}">
            <sys:String>{0:0.00}</sys:String>
            <sys:String>{0:0.0000}</sys:String>
        </x:Array>
        <x:Array x:Key="FormatStringsDoubleHeight_mm_inch" Type="{x:Type sys:String}">
            <sys:String>{0:0.00}</sys:String>
            <sys:String>{0:0.00}</sys:String>
        </x:Array>
        <sys:String x:Key="FormatStringDoubleHeight_number">{0:0}</sys:String>
        <ResourceDictionary x:Key="FileSelector">
            <SolidColorBrush x:Key="TextBoxForegroundThemeBrush" Color="Black"/>
            <SolidColorBrush x:Key="TextBoxBackgroundThemeBrush" Color="#CCFFFFFF"/>
            <SolidColorBrush x:Key="TextBoxSelectionBackgroundThemeBrush" Color="#CCD43E19"/>
            <SolidColorBrush x:Key="TextBoxPointerOverBackgroundThemeBrush" Color="#AAFFFFFF"/>
            <SolidColorBrush x:Key="TextBoxDisabledBackgroundThemeBrush" Color="#CCFFFFFF"/>
            <SolidColorBrush x:Key="TextBoxDisabledForegroundThemeBrush" Color="Black"/>
            <SolidColorBrush x:Key="TextBoxClearTextForegroundThemeBrush" Color="#22000000"/>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Login">
            <SolidColorBrush x:Key="LoginPasswordBackgroundThemeBrush" Color="#FFFFFFFF"/>
            <SolidColorBrush x:Key="LoginPasswordBackgroundBorderBrush" Color="#FFFFFFFF"/>
            <SolidColorBrush x:Key="LoginPasswordCarretBrush" Color="#FFFFFFFF"/>
        </ResourceDictionary>
    </FrameworkElement.Resources>
    <Grid>
    <Grid Name="MainEditor"
          Visibility="{Binding JobEditorVisuEna, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}" >
      <Label Name="SwipeControl" IsManipulationEnabled="true" Content="" Foreground="#00000000"
             Background="#00000000" HorizontalAlignment="Left" Margin="372,0,0,0" VerticalAlignment="Top"
             Width="780" Height="60"/>
      <Label IsManipulationEnabled="true" Content="" Foreground="#00000000" Background="#00000000"
             HorizontalAlignment="Left" Margin="372,40,0,0" VerticalAlignment="Top" Width="476" Height="60"/>
      <Grid Name="Menu" Width="365" HorizontalAlignment="Left" Margin="7,7,0,0"/>
            <Grid Name="Info" Width="500" Height="40" HorizontalAlignment="Right" VerticalAlignment="Top"
            Margin="0,60,18,0">
                <StackPanel Orientation="Horizontal" Width="500">
                    <CustomCtrlLib:Label Width="465" Height="40" BorderThickness="1" HorizontalContentAlignment="Right"
                                Content="{Binding ProductName}"/>
                    <CustomCtrlLib:Label Content=":" Width="10" Height="40" BorderThickness="1" HorizontalContentAlignment="Center"/>
                    <CustomCtrlLib:Label Height="40" BorderThickness="1" HorizontalContentAlignment="Right"
                                Content="{Binding ProductView.ActLayerNum}"/>
                </StackPanel>
            </Grid>
            <Grid Name="Editor" Margin="253,106,12,72">
                <Grid Name="CutSequences"
              Visibility="{Binding CutSeqVisuEna, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}">
                    <CustomCtrlLib:ScrollViewer x:Name="scvCutSeq" HorizontalAlignment="Left" VerticalAlignment="Top" Height="595"
                                       Margin="0,-5,0,0" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto"
                                       PanningMode="VerticalOnly">
                        <ItemsControl Name="CutSeqList" FontSize="22" BorderThickness="0" ScrollViewer.VerticalScrollBarVisibility="Auto"
                          ScrollViewer.HorizontalScrollBarVisibility="Hidden" HorizontalAlignment="Left"
                          ScrollViewer.PanningMode="VerticalOnly" ScrollViewer.PanningDeceleration="0.001"
                          ItemsSource="{Binding CutSequences}">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <CustomCtrlLib:SqRadioButton Name="btnCutSeq" Height="auto" Width="1100" Background="#15000000" GroupName="grpCutSeq"
                                                HorizontalAlignment="Left" VerticalAlignment="Top"
                                                Command="{Binding Path=DataContext.ChooseCutSequenceCommand, ElementName=CutSequences}"
                                                CommandParameter="{Binding}" IsChecked="{Binding IsSelected}">
                                        <Control.Template>
                                            <ControlTemplate TargetType="{x:Type CustomCtrlLib:SqRadioButton}">
                                                <Border Name="tmpBorderCutSequence" BorderThickness="1"
                                Background="{Binding Background, ElementName=btnCutSeq}">
                                                    <ContentPresenter/>
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="ToggleButton.IsChecked" Value="True">
                                                        <Setter TargetName="tmpBorderCutSequence" Property="Border.Background" Value="#35000000"/>
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Control.Template>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
                                            <StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" Height="97" Width="1010"
                                  Margin="0,0,0,0">
                                                <CustomCtrlLib:Label Margin="14,5,0,0" FontSize="18" Foreground="White" HorizontalAlignment="Left"
                                              VerticalAlignment="Top" Content="{Binding Name}"/>
                                                <CustomCtrlLib:Image Name="CutSeqImage" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="21,8,0,0"
                                              Stretch="None" SourceFileName="{Binding Shape}" SourcePath="{Binding ShapePath}"/>
                                            </StackPanel>
                                            <CustomCtrlLib:AppButton x:Name="Confirm" Width="70" Height="70" HorizontalContentAlignment="Center"
                                                VerticalContentAlignment="Center" HorizontalAlignment="Right" Margin="0,0,0,0" Visibility="Hidden"
                                                Command="{Binding Path=DataContext.SelectCutSequenceConfirmCommand, ElementName=CutSequences}"
                                                CommandParameter="{Binding}">
                                                <CustomCtrlLib:Image SourceFileName="Apply.png" Width="40" Height="40"/>
                                            </CustomCtrlLib:AppButton>
                                        </StackPanel>
                                    </CustomCtrlLib:SqRadioButton>
                                    <DataTemplate.Triggers>
                                        <Trigger SourceName="btnCutSeq" Property="ToggleButton.IsChecked" Value="True">
                                            <Setter TargetName="Confirm" Property="UIElement.Visibility" Value="Visible"/>
                                        </Trigger>
                                        <DataTrigger Value="True" Binding="{Binding Path=Used}">
                                            <Setter TargetName="Confirm" Property="UIElement.Visibility" Value="Hidden"/>
                                            <Setter TargetName="btnCutSeq" Property="ToggleButton.IsChecked" Value="true"/>
                                        </DataTrigger>
                                    </DataTemplate.Triggers>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </CustomCtrlLib:ScrollViewer>
                </Grid>
                <Grid Name="Shapes" Margin="6,-6,0,0">
                    <Grid Name="StepLapsPage"
                Visibility="{Binding StepLapVisuEna, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}">
                        <CustomCtrlLib:ListBox Background="#204653" Width="1100" HorizontalAlignment="Left" VerticalAlignment="Top"
                                    ItemsSource="{Binding ProductView.SelectedLayerView.ShapeViews}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <JobEditorControls:StepLapsUserControl/>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <ItemsControl.ItemContainerStyle>
                                <Style TargetType="{x:Type CustomCtrlLib:ListBoxItem}">
                                    <Setter Property="UIElement.Focusable" Value="false"/>
                                </Style>
                            </ItemsControl.ItemContainerStyle>
                        </CustomCtrlLib:ListBox>
                        <Grid Name="StepLapsDefault" Height="146" Width="1098"
                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}" HorizontalAlignment="Left"
                  VerticalAlignment="Bottom">
                            <StackPanel Orientation="Vertical" VerticalAlignment="Top" HorizontalAlignment="Left"
                          Background="{DynamicResource BaseBackgroundThemeBrush}" Margin="-1,0,1,0">
                                <StackPanel Name="NumberOfSteps" Orientation="Horizontal" HorizontalAlignment="Left">
                                    <CustomCtrlLib:AppButton Width="60" Height="48" BorderThickness="0" Margin="0,0,0,0"
                                            Background="{StaticResource Dark1BaseBackgroundThemeBrush}" IsEnabled="false">
                                        <Grid>
                                            <CustomCtrlLib:Image SourceFileName="NumberOfSteps.png" Width="40" Height="40"/>
                                        </Grid>
                                    </CustomCtrlLib:AppButton>
                                    <StackPanel Orientation="Horizontal"
                              Visibility="{Binding EditEnaNumberOfStep, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}">
                                        <CustomCtrlLib:SqRadioButton GroupName="group10" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}" Foreground="White"
                                                  Content="{Binding DefaultPar.StepLapsNumberOfSteps_1}"
                                                  IsChecked="{Binding RBPar.StepLapsNumberOfSteps_1}" Command="{Binding NumberOfStepsParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsNumberOfSteps_1}"/>
                                        <CustomCtrlLib:SqRadioButton GroupName="group10" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsNumberOfSteps_2}"
                                                  IsChecked="{Binding RBPar.StepLapsNumberOfSteps_2}" Command="{Binding NumberOfStepsParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsNumberOfSteps_2}"/>
                                        <CustomCtrlLib:SqRadioButton GroupName="group10" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsNumberOfSteps_3}"
                                                  IsChecked="{Binding RBPar.StepLapsNumberOfSteps_3}" Command="{Binding NumberOfStepsParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsNumberOfSteps_3}"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal"
                              Visibility="{Binding EditEnaNumberOfStep, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}">
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsNumberOfSteps_1, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                        </CustomCtrlLib:TextBox>
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsNumberOfSteps_2, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                        </CustomCtrlLib:TextBox>
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsNumberOfSteps_3, Mode=TwoWay, ValidatesOnNotifyDataErrors=false, StringFormat='    {0}'}">
                                        </CustomCtrlLib:TextBox>
                                    </StackPanel>
                                    <CustomCtrlLib:AppButton Name="EditButtonNumberOfStep" Width="60" Height="48" BorderThickness="0" Margin="1,0,0,0"
                                            Background="{Binding EditButtonBrushNumberOfStep}"
                                            Command="{Binding EditNumberOfStepDefParCommand}">
                                        <CustomCtrlLib:Image SourceFileName="Edit.png" Width="40" Height="40"/>
                                    </CustomCtrlLib:AppButton>
                                    <CustomCtrlLib:SqRadioButton GroupName="group10" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                Visibility="{Binding EditEnaNumberOfStep, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}"
                                                Content="{Binding DPar.StepLapNumberSteps}" IsChecked="{Binding RBPar.StepLapsNumberOfSteps_4}"
                                                Command="{Binding NumberOfStepsParCommand}" CommandParameter="{Binding DPar.StepLapNumberSteps}"/>
                                    <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                          InputScope="Number"
                                          Visibility="{Binding EditEnaNumberOfStep, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}"
                                          Text="{Binding DPar.StepLapNumberSteps, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                    </CustomCtrlLib:TextBox>
                                    <Grid Width="1" Background="{DynamicResource BaseBackgroundThemeBrush}"/>
                                </StackPanel>
                                <StackPanel Name="NumberOfSame" Orientation="Horizontal" HorizontalAlignment="Left" Margin="0,1,0,0">
                                    <CustomCtrlLib:AppButton Width="60" Height="48" BorderThickness="0" Margin="0,0,0,0"
                                            Background="{StaticResource Dark1BaseBackgroundThemeBrush}" IsEnabled="false">
                                        <Grid>
                                            <CustomCtrlLib:Image SourceFileName="NumberOfSame.png" Width="40" Height="40"/>
                                        </Grid>
                                    </CustomCtrlLib:AppButton>
                                    <StackPanel Orientation="Horizontal"
                              Visibility="{Binding EditEnaNumberOfSame, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}">
                                        <CustomCtrlLib:SqRadioButton GroupName="group20" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsNumberOfSame_1}"
                                                  IsChecked="{Binding RBPar.StepLapsNumberOfSame_1}" Command="{Binding NumberOfSameParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsNumberOfSame_1}"/>
                                        <CustomCtrlLib:SqRadioButton GroupName="group20" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsNumberOfSame_2}"
                                                  IsChecked="{Binding RBPar.StepLapsNumberOfSame_2}" Command="{Binding NumberOfSameParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsNumberOfSame_2}"/>
                                        <CustomCtrlLib:SqRadioButton GroupName="group20" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsNumberOfSame_3}"
                                                  IsChecked="{Binding RBPar.StepLapsNumberOfSame_3}" Command="{Binding NumberOfSameParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsNumberOfSame_3}"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal"
                              Visibility="{Binding EditEnaNumberOfSame, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}">
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsNumberOfSame_1, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                        </CustomCtrlLib:TextBox>
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsNumberOfSame_2, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                        </CustomCtrlLib:TextBox>
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsNumberOfSame_3, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                        </CustomCtrlLib:TextBox>
                                    </StackPanel>
                                    <CustomCtrlLib:AppButton Name="EditButtonNumberOfSame" Width="60" Height="48" BorderThickness="0" Margin="1,0,0,0"
                                            BorderBrush="{StaticResource BaseBackgroundThemeBrush}"
                                            Background="{Binding EditButtonBrushNumberOfSame}"
                                            Command="{Binding EditNumberOfSameDefParCommand}">
                                        <CustomCtrlLib:Image SourceFileName="Edit.png" Width="40" Height="40"/>
                                    </CustomCtrlLib:AppButton>
                                    <CustomCtrlLib:SqRadioButton GroupName="group20" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                Visibility="{Binding EditEnaNumberOfSame, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}"
                                                Content="{Binding DPar.StepLapNumberSame}" IsChecked="{Binding RBPar.StepLapsNumberOfSame_4}"
                                                Command="{Binding NumberOfSameParCommand}" CommandParameter="{Binding DPar.StepLapNumberSame}"/>
                                    <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                          InputScope="Number"
                                          Visibility="{Binding EditEnaNumberOfSame, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}"
                                          Text="{Binding DPar.StepLapNumberSame, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, StringFormat='    {0}'}">
                                    </CustomCtrlLib:TextBox>
                                    <Grid Width="1" Background="{DynamicResource BaseBackgroundThemeBrush}"/>
                                </StackPanel>
                                <StackPanel Name="StapLapValue" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top"
                            Margin="0,1,0,0">
                                    <CustomCtrlLib:AppButton Width="60" Height="48" BorderThickness="0" Margin="0,0,0,0"
                                            Background="{StaticResource Dark1BaseBackgroundThemeBrush}" IsEnabled="false">
                                        <Grid>
                                            <CustomCtrlLib:Image SourceFileName="StepValue.png" Width="40" Height="40"/>
                                        </Grid>
                                    </CustomCtrlLib:AppButton>
                                    <StackPanel Orientation="Horizontal"
                              Visibility="{Binding EditEnaStepLapValue, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}">
                                        <CustomCtrlLib:SqRadioButton GroupName="group30" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsValue_1, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                  IsChecked="{Binding RBPar.StepLapsValue_1}" Command="{Binding StepLapValueParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsValue_1}"/>
                                        <CustomCtrlLib:SqRadioButton GroupName="group30" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsValue_2, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                  IsChecked="{Binding RBPar.StepLapsValue_2}" Command="{Binding StepLapValueParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsValue_2}"/>
                                        <CustomCtrlLib:SqRadioButton GroupName="group30" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                  Content="{Binding DefaultPar.StepLapsValue_3, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                  IsChecked="{Binding RBPar.StepLapsValue_3}" Command="{Binding StepLapValueParCommand}"
                                                  CommandParameter="{Binding DefaultPar.StepLapsValue_3}"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal"
                              Visibility="{Binding EditEnaStepLapValue, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}">
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsValue_1, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble2}}">
                                        </CustomCtrlLib:TextBox>
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsValue_2, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble2}}">
                                        </CustomCtrlLib:TextBox>
                                        <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                            InputScope="Number"
                                            Text="{Binding DefaultPar.StepLapsValue_3, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble2}}">
                                        </CustomCtrlLib:TextBox>
                                    </StackPanel>
                                    <CustomCtrlLib:AppButton Name="EditButtonStepLapValue" Width="60" Height="48" BorderThickness="0" Margin="1,0,0,0"
                                            BorderBrush="{StaticResource BaseBackgroundThemeBrush}"
                                            Background="{Binding EditButtonBrushStepLapValue}"
                                            Command="{Binding EditStepLapValueDefParCommand}">
                                        <CustomCtrlLib:Image SourceFileName="Edit.png" Width="40" Height="40"/>
                                    </CustomCtrlLib:AppButton>
                                    <CustomCtrlLib:SqRadioButton GroupName="group30" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                Visibility="{Binding EditEnaStepLapValue, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}"
                                                Content="{Binding DPar.StepLapStepValue, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                IsChecked="{Binding RBPar.StepLapsValue_4}" Command="{Binding StepLapValueParCommand}"
                                                CommandParameter="{Binding DPar.StepLapStepValue}"/>
                                    <CustomCtrlLib:TextBox Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0" HorizontalContentAlignment="Center"
                                          InputScope="Number"
                                          Visibility="{Binding EditEnaStepLapValue, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}"
                                          Text="{Binding DPar.StepLapStepValue, Mode=TwoWay, ValidatesOnNotifyDataErrors=true, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble2}}">
                                    </CustomCtrlLib:TextBox>
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </Grid>
                    <Grid Name="TipCutsPage"
                Visibility="{Binding TipCutVisuEna, Converter={StaticResource BoolToVisibility}, ConverterParameter=false}">
                        <CustomCtrlLib:ListBox Background="#204653" Width="1100" HorizontalAlignment="Left" VerticalAlignment="Top"
                                    ItemsSource="{Binding ProductView.SelectedLayerView.ShapeViews}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <JobEditorControls:TipCutsUserControl/>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <ItemsControl.ItemContainerStyle>
                                <Style TargetType="{x:Type CustomCtrlLib:ListBoxItem}">
                                    <Setter Property="UIElement.Focusable" Value="false"/>
                                </Style>
                            </ItemsControl.ItemContainerStyle>
                        </CustomCtrlLib:ListBox>
                        <Grid Name="TipCutsDefault" Height="146" Width="1098"
                  Background="{StaticResource Dark1BaseBackgroundThemeBrush}" HorizontalAlignment="Left"
                  VerticalAlignment="Bottom">
                            <StackPanel Orientation="Vertical" VerticalAlignment="Top" HorizontalAlignment="Left">
                                <StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top"
                            Background="{DynamicResource BaseBackgroundThemeBrush}">
                                    <StackPanel Name="TipCutsHeight" Orientation="Horizontal" HorizontalAlignment="Left">
                                        <CustomCtrlLib:AppButton Width="60" Height="48" BorderThickness="0" Margin="0,0,0,0"
                                              Background="{StaticResource Dark1BaseBackgroundThemeBrush}" IsEnabled="false">
                                            <Grid>
                                                <CustomCtrlLib:Image SourceFileName="TipCutHeight.png" Width="40" Height="40"/>
                                            </Grid>
                                        </CustomCtrlLib:AppButton>
                                        <StackPanel Orientation="Horizontal"
                                Visibility="{Binding EditEnaTipCutHeight, Converter={StaticResource BoolToVisibility}, ConverterParameter=true}">
                                            <CustomCtrlLib:SqRadioButton GroupName="group55" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                    Background="{StaticResource Dark1BaseBackgroundThemeBrush}" Foreground="White"
                                                    Content="{Binding DefaultPar.TipCutsHeight_1, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                    IsChecked="{Binding RBPar.TipCutsHeight_1}" Command="{Binding TipCutHeightParCommand}"
                                                    CommandParameter="{Binding DefaultPar.TipCutsHeight_1}"/>
                                            <CustomCtrlLib:SqRadioButton GroupName="group55" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                    Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                    Content="{Binding DefaultPar.TipCutsHeight_2, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                    IsChecked="{Binding RBPar.TipCutsHeight_2}" Command="{Binding TipCutHeightParCommand}"
                                                    CommandParameter="{Binding DefaultPar.TipCutsHeight_2}"/>
                                            <CustomCtrlLib:SqRadioButton GroupName="group55" Width="135" Height="48" Margin="1,0,0,0" BorderThickness="0"
                                                    Background="{StaticResource Dark1BaseBackgroundThemeBrush}"
                                                    Content="{Binding DefaultPar.TipCutsHeight_3, Converter={StaticResource converterDoubleToString}, ConverterParameter={StaticResource FormatStringsDouble1}}"
                                                    IsChecked="{Binding RBPar.TipCutsHeight_3}" Command="{Binding TipCutHeightParCommand}"
